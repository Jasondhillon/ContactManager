{
  "name": "mern-contact-manager",
  "version": "2.0.0",
  "description": "Contact Manager built with MERN",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Jason Dhillon",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^4.1.2",
    "config": "^3.2.2",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.11",
    "package.json": "^2.0.1",
    "react": "^16.9.0",
    "react-scripts": "^3.1.1"
  },
  "devDependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.3.1",
    "nodemon": "^1.19.2",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-transition-group": "^4.3.0",
    "reactstrap": "^8.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  }
}
